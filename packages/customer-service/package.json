{
  "name": "@icommerce/customer-service",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "dev": "DEBUG=customer-service:* npm run dev:compile",
    "dev:compile": "rm -rf dist && webpack --config webpack.server.js --progress --watch",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix --color ./src",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo"
  },
  "dependencies": {
    "@lib/common": "*",
    "@lib/microservice": "*",
    "@lib/server": "*"
  }
}
